{
    "manifest_version":3,
    "version":"1.0",
    "name":"flownotes",
    
    "action":{
        "default_popup":"index.html",
        "default_icon":"logo.png"
    },
    "icons":{
    "128":"logo.png"
    },
    "permissions":[
        "activeTab","storage"
    ],
    "content_scripts": [{
  "matches": ["<all_urls>"],
  "js": ["content.js"],
  "run_at": "document_idle"
  
}]

}